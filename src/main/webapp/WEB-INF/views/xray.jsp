<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Medical Dashboard</title>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap" rel="stylesheet">
        <link href="\CSS\xray.css" rel="stylesheet" type="text/css">
</head>
<body>
    <div class="container">
        <!-- Sidebar -->
        <div class="sidebar">
            <div class="sidebar-icon">Adm</div>
            <div class="sidebar-buttons">
                <div class="sidebar-icon">1</div>
                <div class="sidebar-icon">2</div>
                <div class="sidebar-icon">3</div>
                <div class="sidebar-icon">4</div>
            </div>
            <div class="sidebar-icon">5</div>
        </div>

        <!-- Main Content -->
        <div class="main-content">
            <!-- Toolbar -->
            <div class="toolbar">
                <button class="toolbar-button">Button 1</button>
                <button class="toolbar-button">Button 2</button>
                <button class="toolbar-button">Button 3</button>
                <button class="toolbar-button">Button 4</button>
                <button class="toolbar-button">Button 5</button>
                <button class="toolbar-button">Button 6</button>
                <button class="toolbar-button">Button 7</button>
                <button class="toolbar-button">Button 8</button>
                <button class="toolbar-button">Button 9</button>
                <button class="toolbar-button">Button 10</button>
                <button class="toolbar-button">Button 11</button>
                <button class="toolbar-button">Button 12</button>
                <button class="toolbar-button">Button 13</button>
            </div>

            <!-- Content Area -->
            <div class="content-area">
                <div class="content-box">
                    <img src="https://via.placeholder.com/300" alt="Medical Image" class="content-image">
                </div>
                <div class="content-box"></div>
                <div class="content-box"></div>
                <div class="content-box"></div>
            </div>
        </div>
    </div>
</body>
</html>